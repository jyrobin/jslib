{
  "name": "@jyrobin/jslib",
  "type": "module",
  "version": "0.1.0",
  "description": "A sampling of js utilities in typescript",
  "scripts": {
    "build": "yarn tsc --build",
    "clean": "yarn tsc --build --clean",
    "watch": "yarn tsc --build --watch",
    "test": "tsm ../node_modules/uvu/bin.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jyrobin/jslib.git"
  },
  "author": "Jing-Ying Chen <jy.robin.chen@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jyrobin/jslib/issues"
  },
  "homepage": "https://github.com/jyrobin/jslib#readme",
  "dependencies": {
    "format-util": "latest"
  },
  "devDependencies": {
    "@types/debug": "latest",
    "@types/format-util": "latest",
    "uvu": "latest",
    "tsm": "latest",
    "typescript": "latest",
    "ts-node": "latest"
  },
  "main": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*/index.js"
  }
}
